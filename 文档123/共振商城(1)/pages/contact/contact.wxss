/* pages/contact/contact.wxss */

.contact{
  width: 100%;
  overflow: hidden;
   padding-bottom: 60px;
}
/*-------------------标题 开始---------------------*/
.contact>.title{
  width: 90%;
  margin: auto;
  height:136.8rpx;

  display: flex;
  align-items: center; 
  justify-content: center;
}
.contact .title .wz{
  text-align: center;
  margin: 0 5px;
}
.contact .title .wz .yw{
color: #545353;
font-family: '微软雅黑';
font-size:40rpx;
font-weight: 100;
padding: 0 5px;
display: -webkit-box;
  word-break: break-all;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-box-orient: vertical;
 -webkit-line-clamp:1;
}
.contact .title .wz .zw{
color: #545353;
font-family: '微软雅黑';
font-size:45rpx;
}
.contact .title .zuo{
  overflow: hidden;
  width:60px;
  height: 70px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.contact .title .zuo .xian{
  width:80%;
  height: 1px;
  background: #d0d0d0;
  margin-top: -63rpx; 
  margin-left: -5rpx;
}
.contact .title .zuo .yuan{
  font-family: '微软雅黑';
  font-size: 15rpx;
  color: #d0d0d0;
  margin-top: -65rpx;
  margin-left: 5rpx;
}

.contact .title .you{
  overflow: hidden;
  width:60px;
  height: 70px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.contact .title .you .xian{
  width:80%;
  height: 1px;
  background: #d0d0d0;
  margin-top:-63rpx; 
  margin-left:5rpx;
}
.contact .title .you .yuan{
 font-family: '微软雅黑';
  font-size: 15rpx;
  color: #d0d0d0;
  margin-top: -65rpx;
  margin-left: 5rpx;
}


/*---------------------联系方式 开始---------------------*/
.lxfs{
  width: 100%;
  overflow: hidden;
}

.biaot{
  width:590rpx;
  height:94rpx;
  margin: auto;
   text-align: center;
  margin-top: 50rpx;
}

.biaot text{
  font-family: '微软雅黑';
  font-size:35rpx;
  color: #545353;
  font-weight: 200;
  line-height: 94rpx;
}

.lxfs .nier{
  width:75%;
  margin: auto;
  margin-bottom: 20px;
}

.lxfs .nier text{
  font-family: '微软雅黑';
  font-size:32rpx;
  color: #545353;
  display: block;
  margin-top: 40rpx;
  border-bottom: 1px #d0d0d0 dashed;
  padding-bottom: 10rpx;
}

/*---------------------在线留言 开始----------------------*/
.contact .message{
  width:78%;
  margin: auto;
  margin-bottom: 20px;
}

.contact .message form .kuang{
 display: flex;
  justify-content: space-around;
  margin: 10px 0;
  width: 100%;
  height: 30px;
}
.contact .message form .kuang text{
  width: 20%;
  font-family: "微软雅黑";
  font-size:34rpx;
  color: #545353;
  margin-top:10rpx;
  display: block;
}

.contact .message form .kuang .input{
  width:80%;
  height:30px;
  background:#fbfbfb;
  font-family: "微软雅黑";
  font-size:30rpx;
  color:#545353;
  padding: 0 10rpx;
}

.contact .message form .kuang .phcolor{
  color: #ececec;
}

.contact .message form .liuyan{
 display: flex;
  justify-content: space-around;
  margin: 10px 0;
  width: 100%;
  height: 30px;
  margin-bottom:56px;
}
.contact .message form .liuyan text{
  width: 20%;
  font-family: "微软雅黑";
  font-size:34rpx;
  color: #545353;
  margin-top:10rpx;
  display: block;
}

.contact .message form .liuyan .textares{
  width:80%;
  height:60px;
  background:#fbfbfb;
  font-family: "微软雅黑";
  font-size:30rpx;
  color:#545353;
  padding: 20rpx 10rpx;
}

.contact .message form .liuyan .phcolor{
  color: #ececec;
}

.contact .message form .anniu{
  width:85%;
  margin-left:102rpx;
  display: flex;
  justify-content: space-around;
}
.contact .message form .anniu .cc{
  width:45%;
  height:80rpx;
  font-family: '微软雅黑';
  font-size:35rpx;
  background:#f20000;
  color: #fff;
}

.contact .message form .anniu .tj{
  width:45%;
  height:80rpx;
  font-family: '微软雅黑';
  font-size:35rpx;
  background:#00e610;
  color: #fff;
}

.contact .dt{
  width: 78%;
  margin: auto;
}

.contact .dt image{
  width: 100%;
}















